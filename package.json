{
  "name": "acorn",
  "version": "1.0.0",
  "description": "To increase efficiency in FSC Certification",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "nodemon --exec babel-node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client",
    "build": "babel server.js --out-file server.compiled.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.4",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "concurrently": "^4.1.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "docusign-esign": "^5.6.0",
    "ejs": "^3.1.5",
    "express": "^4.16.4",
    "express-flash": "0.0.2",
    "express-session": "^1.17.1",
    "fs-extra": "^9.0.1",
    "gravatar": "^1.8.0",
    "helmet": "^4.1.0",
    "jsonwebtoken": "^8.5.0",
    "memorystore": "^1.6.2",
    "moment": "^2.27.0",
    "node-cron": "^2.0.3",
    "passport": "^0.4.1",
    "passport-docusign": "^1.3.0",
    "path": "^0.12.7",
    "react-addons-css-transition-group": "^15.6.2",
    "request": "^2.88.2",
    "validator": "^10.11.0"
  }
}
